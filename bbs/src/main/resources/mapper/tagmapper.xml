<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "- //mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bbs.mapper.HashTagMapper">
    <insert id="insertTag" useGeneratedKeys="true" keyProperty="id" parameterType="hashtag">
        INSERT INTO TAG(CONTENT)
        VALUES (#{content});
    </insert>
    <select id="findByHashTag" resultType="hashtag">
        select * from tag where content = #{content};
    </select>
</mapper>